{
  "name": "@1up/static-starter",
  "version": "0.0.1",
  "main": "dist/scripts/app.bundle.js",
  "scripts": {
    "clear:app": "rimraf dist/**",
    "prestart": "npm run clear:app",
    "local:watch": "node tasks/watchers/watchers.js",
    "local:server": "node tasks/mangony/mangony-server.js",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel local:*",
    "prebuild": "npm run clear:app",
    "build": "rollup -c && npm run html:generate && npm run css:generate",
    "dev:build": "cross-env NODE_ENV=development npm run build",
    "prod:build": "cross-env NODE_ENV=production npm run build && npm run gzip",
    "gzip": "gzip ./lib/**/*.js && gzip ./lib-umd/**/*.js",
    "analyze:bundle": "source-map-explorer ./dist/scripts/app.bundle.js",
    "prettify:scripts": "prettier --write \"src/**/*.{ts, test.ts}\"",
    "test": "jest --runInBand --no-cache --config ./jest.config.js",
    "sprite:generate": "node tasks/icons/sprite.js",
    "webfont:generate": "node tasks/icons/webfont.js",
    "grunticon:generate": "node tasks/icons/grunticon.js",
    "css:generate": "node tasks/sass/sass-compiler.js",
    "css:separate": "node tasks/css-separation/css-separation.js",
    "images:generate": "node tasks/images/images-resizer.js",
    "images:export": "node tasks/images/images-export.js",
    "faker": "node tasks/faker/faker.js",
    "html:generate": "node tasks/mangony/mangony-build.js",
    "lint:styles": "stylelint src/app/**/*.scss --config tasks/linting/stylelint.config.js",
    "lint:scripts": "eslint \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prod:build && lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettify:scripts",
      "npm run lint:scripts",
      "git add"
    ]
  },
  "dependencies": {
    "@veams/base": "^1.0.3",
    "@veams/component": "^1.3.1",
    "@veams/core": "^1.1.1",
    "@veams/helpers": "^1.4.1",
    "@veams/http-service": "^1.0.2",
    "@veams/plugin-logger": "^1.3.1",
    "@veams/plugin-media-query-handler": "^1.1.0",
    "@veams/plugin-modules": "^1.2.0",
    "@veams/query": "^3.0.0",
    "@veams/vent": "^1.0.0",
    "lazysizes": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-do-expressions": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.5",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "bourbon": "^7.0.0",
    "bourbon-neat": "^4.0.0",
    "browser-sync": "^2.26.13",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.1",
    "cross-env": "^7.0.2",
    "dr-svg-sprites": "^1.0.0",
    "esbuild": "^0.8.20",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.0.1",
    "gzip-cli": "^1.1.1",
    "husky": "^5.0.4",
    "include-media": "^1.4.9",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.5.1",
    "mangony": "^2.0.0-alpha.9",
    "mangony-hbs-helpers": "^0.7.0",
    "node-sass": "^5.0.0",
    "node-sass-magic-importer": "^5.3.2",
    "npm-run-all": "^4.1.1",
    "postcss": "^8.1.14",
    "postcss-cssnext": "^3.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.0",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "source-map-explorer": "^2.5.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "author": "Sebastian Fitzner",
  "license": "MIT"
}
